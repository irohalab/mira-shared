   "keywords": [],
   "author": "Everett Summer",
   "license": "Apache-2.0",
   "dependencies": {
      "@cloudamqp/amqp-client": "^3.2.1",
      "@mikro-orm/core": "^6.4.16",
      "@mikro-orm/postgresql": "^6.4.16",
      "@sentry/node": "^7.0.0",
      "amqplib": "^0.10.3",
      "inversify": "^6.0.2",
      "inversify-binding-decorators": "^4.0.0",
      "inversify-express-utils": "^6.4.6",
      "inversify-logger-middleware": "^3.1.0",
      "pg": "^8.12.0",
      "pino": "^8.19.0",
      "rascal": "^19.0.0",
      "reflect-metadata": "^0.2.2",
      "uuid": "^9.0.1"
   },
   "devDependencies": {
      "@mikro-orm/migrations": "^6.4.16",
      "@types/amqplib": "^0.10.5",
      "@types/express": "^4.17.21",
      "@types/node": "^24.1.0",
      "@types/rascal": "^10.2.1",
      "@types/supertest": "^2.0.16",
      "ava": "^6.1.2",
      "cross-env": "^7.0.3",
      "rimraf": "^5.0.5",
      "supertest": "^6.3.4",
      "ts-node": "^10.9.2",
      "tslint": "^6.1.3",
      "typescript": "^5.5.4"
   },
   "engines": {
      "node": ">=20.0.0"
   },
   "ava": {
      "extensions": [
         "ts"
      ],
      "files": [
         "src/services/*.spec.ts"
      ],
      "require": [
         "ts-node/register"
      ],
      "verbose": true
   },
   "publishConfig": {
      "access": "public",
      "registry": "https://registry.npmjs.org/"
   }
